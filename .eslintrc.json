{
  "extends": ["airbnb", "airbnb-typescript", "airbnb/hooks"],
  "parserOptions": {
    "project":"./tsconfig.json"
  },
  "rules": {
    "react/prop-types": "error",
    "react/require-default-props": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ],
    "semi": "off",
    "@typescript-eslint/semi": [2,"never"]
  }
}